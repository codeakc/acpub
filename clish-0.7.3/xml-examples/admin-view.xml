<?xml version="1.0" encoding="UTF-8"?>
<CLISH_MODULE xmlns="http://clish.sourceforge.net/XMLSchema" 
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:schemaLocation="http://clish.sourceforge.net/XMLSchema
                     http://clish.sourceforge.net/XMLSchema/clish.xsd">
    <VIEW name="admin-view" prompt="admin# ">

        <COMMAND name="exit"
                 help="Go back to main menu"
                 view="root-view">
		<ACTION>echo "Leaving admin-view" </ACTION>
        </COMMAND>
		<COMMAND name="create" help="Create" />
		<COMMAND name="create config-element" help="Create a new config element">
				<PARAM name="cename" help="Config element name" ptype="STRING" />
				<PARAM name="cetype" help="Config element type" ptype="CETYPE" />
				<PARAM name="celen" help="Config element length in bytes" ptype="UINT" />
			<ACTION builtin="clish_usr_cb">"config-element" 1 ${cename} ${cetype} ${celen}</ACTION>
		</COMMAND>
    </VIEW>

</CLISH_MODULE>
